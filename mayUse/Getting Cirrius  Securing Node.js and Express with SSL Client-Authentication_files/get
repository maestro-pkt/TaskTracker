var SiteSealBadge = new Object(); 
SiteSealBadge.display=function() 
{ 
  var domainName = "gettingcirrius.com"; 
  var baseUrl = "http://tracedseals.starfieldtech.com/siteseal"; 
  var sealId = "55e4ye7y7mb73166965d924061880epy5dy7mb7355e4ye7c8a57def5b8f6ff79"; 
  var refererUrl = ""; 
  var cdSealType = "Seal1"; 
  var scpt = document.getElementById("cdSiteSeal1"); 
  var imageName = "siteseal_base_lite.gif"; 
  var link = document.createElement("a"); 
  link.setAttribute("href", "javascript:void(0);"); 
  link.setAttribute("id", "siteSealFauxBadge"); 
  link.style.textDecoration = "none"; 
  link.style.fontFamily = "Arial, Verdana, Helvetica"; 
  link.onclick = function() 
  { 
    var url = baseUrl + "/certsealnew?cdSealType=" + cdSealType + "&sealId=" + sealId + "&referer=" + refererUrl; 
    var pop = null; 
    try 
    { 
      pop = window.open(url, 'SealVerification', 'width=600,height=635,left=0,top=0,toolbar=no,location=yes,scrollbars=yes,status=yes,resizable=yes,fullscreen=no') 
      if (pop != null && pop != undefined) 
      { 
        pop.focus(); 
      } 
    } 
    catch (err) 
    { 
      pop = null; 
    } 
    if (pop == null || pop == undefined) 
    { 
      if (confirm("You appear to have a popup blocker installed. Would you like to view the verification details for this site?")) 
      { 
        document.location.href = url; 
      } 
    }
  }; 
  var divBadge = document.createElement("div"); 
  divBadge.align = "center"; 
  divBadge.style.borderWidth = "0px"; 
  divBadge.style.backgroundImage = "url('http://tracedseals.starfieldtech.com/siteseal/images/siteseal_base_lite.gif')"; 
  divBadge.style.backgroundRepeat = "no-repeat"; 
  divBadge.style.cursor = "pointer"; 
  divBadge.style.width = "132px"; 
  divBadge.style.height = "31px"; 
  link.appendChild(divBadge); 
  divBadge.setAttribute("alt", "Click to Verify"); 
  divBadge.setAttribute("title", "Click to Verify"); 
  var outerDateDiv = document.createElement("div"); 
  outerDateDiv.style.position = "relative"; 
  outerDateDiv.style.top = "20px"; 
  outerDateDiv.style.left = "12px"; 
  outerDateDiv.style.width = "106px"; 
  outerDateDiv.style.lineHeight = "7.5pt"; 
  var divOrigDate = document.createElement("div"); 
  divOrigDate.style.color = "#6a9cba"; 
  divOrigDate.style.fontWeight = "bold"; 
  divOrigDate.style.fontSize = "6pt"; 
  divOrigDate.innerHTML = "2011-01-13"; 
  outerDateDiv.appendChild(divOrigDate); 
  divBadge.appendChild(outerDateDiv); 
  scpt.parentNode.insertBefore(link, scpt); 
  var list = scpt.getElementsByTagName("a"); 
  for (idx = list.length - 1; idx >= 0; idx--)
  {
    if (list[idx].id == "gdUrl")
    {
      scpt.removeChild(list[idx]);
    }
  }
}; 
if (window.addEventListener) 
{ 
 window.addEventListener("load", SiteSealBadge.display, false); 
} 
else 
{ 
 window.attachEvent("onload", SiteSealBadge.display); 
} 
